#include <bits/stdc++.h>
using namespace std;
void printF(int ind, int arr[], vector<int> &ds, int n){
    
    if(ind == n){
        for(auto it: ds){
            cout<<it<<" ";
        }
        if(ds.size() ==0){
            cout<<"{}";
        }
        
        cout<<endl;
        return;
    }
    // take the particular index
    ds.push_back(arr[ind]);
    printF(ind+1,arr,ds,n);
    ds.pop_back();
    
    //not take
    printF(ind+1, arr, ds, n);
    
    
    
}
int main() {
	// your code goes here
	int arr[]={3,1,2};
	int n=3;
	vector<int> ds;
	printF(0,arr,ds,n);
	return 0;
}
///Printing all subsequences
